{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true,
        "**/bin": true,
        "**/obj": true,
        "__blobstorage__": true,
        "__queuestorage__": true,
        "__azurite_db_*": true,
